RequestBranchModal></PromoRequestBranchModal>\n  </div>\n</template>\n<script>\nimport Vue from \"vue\";\nimport axiosClient from \"../../../../axios\";\nimport PromoRequestBranchModal from \"./PromoRequestBranchModal.vue\";\n\nexport default {\n  props: [\"promoData\"],\n  components: {\n    PromoRequestBranchModal,\n  },\n  watch: {},\n  computed: {\n    /***\n     * Searches through table data and returns a paginated array.\n     * Note that this should not be used for table with a lot of data as it might be slow!\n     * Do the search and the pagination on the server and display the data retrieved from server instead.\n     * @returns {computed.pagedData}\n     */\n  },\n  data() {\n    return {};\n  },\n  mounted() {},\n  methods: {\n    confirm(x) {\n      this.$emit(\"confirm\", x);\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\">\n.tr_num {\n  font-size: 35px;\n  font-weight: 700;\n}\n</style>\n"],"mappings":"AA2CA,OAAAA,GAAA;AACA,OAAAC,WAAA;AACA,OAAAC,uBAAA;AAEA;EACAC,KAAA;EACAC,UAAA;IACAF;EACA;EACAG,KAAA;EACAC,QAAA,GAOA;EACAC,KAAA;IACA;EACA;EACAC,QAAA;EACAC,OAAA;IACAC,QAAAC,CAAA;MACA,KAAAC,KAAA,YAAAD,CAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}
  this.isEditParagraph();\n  },\n  methods: {\n    isEditParagraph() {\n      if (\n        sessionStorage.getItem(\"Position\") == \"Reviewer\" ||\n        sessionStorage.getItem(\"Position\") == \"Approver\"\n      ) {\n        this.isSubmit = false;\n      }\n    },\n    reloadPage() {\n      const submitdatemodal = new bootstrap.Modal(\"#submiteditauthorizeddatemodal\");\n      submitdatemodal.show();\n      // if (\n      //   sessionStorage.getItem(\"Position\") == \"Reviewer\" ||\n      //   sessionStorage.getItem(\"Position\") == \"Approver\"\n      // )\n      //   this.$router.push({\n      //     path: \"/pull-out/requests\",\n      //   });\n      // else\n      //   this.$router.push({\n      //     path: \"/pull-out/pullout-transaction\",\n      //   });\n    },\n    counterSaving() {\n      setTimeout(() => {\n        this.counter = false;\n      }, this.savingCounter * 1000);\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\">\n.tr_num {\n  font-size: 35px;\n  font-weight: 700;\n}\n</style>\n"],"mappings":"AAiDA,OAAAA,6BAAA;AAEA;EACAC,KAAA;EACAC,UAAA;IAAAF,6BAAA,EAAAA;EAAA;EACAG,KAAA;IACAC,aAAA;EACA;EACAC,QAAA,GAOA;EACAC,IAAA,WAAAA,KAAA;IACA;MACAC,OAAA;MACAC,QAAA;IACA;EACA;EACAC,OAAA,WAAAA,QAAA;IACA,KAAAC,eAAA;EACA;EACAC,OAAA;IACAD,eAAA,WAAAA,gBAAA;MACA,IACAE,cAAA,CAAAC,OAAA,8BACAD,cAAA,CAAAC,OAAA,4BACA;QACA,KAAAL,QAAA;MACA;IACA;IACAM,UAAA,WAAAA,WAAA;MACA,IAAAC,eAAA,OAAAC,SAAA,CAAAC,KAAA;MACAF,eAAA,CAAAG,IAAA;IAYA;IACAC,aAAA,WAAAA,cAAA;MAAA,IAAAC,KAAA;MACAC,UAAA;QACAD,KAAA,CAAAb,OAAA;MACA,QAAAH,aAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}
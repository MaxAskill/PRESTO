{"ast":null,"code":"import AppNavbar from \"./Layout/AppNavbar\";\nimport { Card, Checkbox, Button, InfoSection } from \"src/components/UIComponents\";\nexport default {\n  components: {\n    Card,\n    AppNavbar,\n    InfoSection,\n    [Checkbox.name]: Checkbox,\n    [Button.name]: Button\n  },\n  data() {\n    return {\n      form: {\n        firstName: \"\",\n        lastName: \"\",\n        email: \"\",\n        password: \"\",\n        acceptTerms: true\n      }\n    };\n  },\n  methods: {\n    toggleNavbar() {\n      document.body.classList.toggle(\"nav-open\");\n    },\n    closeMenu() {\n      document.body.classList.remove(\"nav-open\");\n      document.body.classList.remove(\"off-canvas-sidebar\");\n    },\n    register() {\n      axiosClient.post(\"/register\", {\n        firstName: this.formfirstName,\n        lastName: this.form.lastName,\n        email: this.form.email,\n        password: this.form.password\n      }).then(response => {\n        sessionStorage.setItem(\"UserID\", response.data.user.id);\n        sessionStorage.setItem(\"Token\", response.data.token);\n        sessionStorage.setItem(\"Position\", response.data.user.position);\n        this.$router.push({\n          name: \"Overview\"\n        });\n      }).catch(error => {\n        console.error(error);\n      });\n    }\n  },\n  beforeDestroy() {\n    this.closeMenu();\n  }\n};","map":{"version":3,"names":["AppNavbar","Card","Checkbox","Button","InfoSection","components","name","data","form","firstName","lastName","email","password","acceptTerms","methods","toggleNavbar","document","body","classList","toggle","closeMenu","remove","register","axiosClient","post","formfirstName","then","response","sessionStorage","setItem","user","id","token","position","$router","push","catch","error","console","beforeDestroy"],"sources":["src/components/Dashboard/Views/Pages/Register.vue"],"sourcesContent":["<template>\n  <div class=\"register-page\">\n    <app-navbar></app-navbar>\n    <div class=\"wrapper wrapper-full-page\">\n      <div class=\"full-page register-page section-image\" filter-color=\"black\">\n        <div class=\"content\">\n          <div class=\"container\">\n            <div class=\"col-lg-4 col-md-6 ml-auto mr-auto\">\n              <card type=\"signup\" class=\"text-center\">\n                <template slot=\"header\">\n                  <h4 class=\"card-title\">Register</h4>\n                </template>\n\n                <fg-input\n                  v-model=\"form.firstName\"\n                  addon-left-icon=\"nc-icon nc-single-02\"\n                  placeholder=\"Full Name...\"\n                ></fg-input>\n                <fg-input\n                  v-model=\"form.email\"\n                  addon-left-icon=\"nc-icon nc-email-85\"\n                  placeholder=\"Email...\"\n                ></fg-input>\n                <el-select\n                  class=\"select-default\"\n                  size=\"large\"\n                  placeholder=\"Select Company\"\n                  v-model=\"form.company\"\n                >\n                  <el-option>NBFI</el-option>\n                  <!-- <el-option\n                  v-for=\"option in companyList\"\n                  class=\"select-default\"\n                  :value=\"option.shortName\"\n                  :label=\"option.shortName\"\n                  :key=\"option.id\"\n                >\n                </el-option> -->\n                </el-select>\n                <fg-input\n                  v-model=\"form.password\"\n                  addon-left-icon=\"nc-icon nc-key-25\"\n                  placeholder=\"Password\"\n                  type=\"password\"\n                ></fg-input>\n                <fg-input\n                  v-model=\"form.confirmPassword\"\n                  addon-left-icon=\"nc-icon nc-key-25\"\n                  placeholder=\"Confirm Password\"\n                  type=\"password\"\n                ></fg-input>\n\n                <p-button slot=\"footer\" type=\"info\" round>Register</p-button>\n              </card>\n            </div>\n          </div>\n        </div>\n        <!-- <app-footer></app-footer> -->\n        <div\n          class=\"full-page-background\"\n          style=\"background-image: url(static/img/background/jan-sendereks.jpg)\"\n        ></div>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nimport AppNavbar from \"./Layout/AppNavbar\";\n// import AppFooter from './Layout/AppFooter'\nimport { Card, Checkbox, Button, InfoSection } from \"src/components/UIComponents\";\n\nexport default {\n  components: {\n    Card,\n    AppNavbar,\n    // AppFooter,\n    InfoSection,\n    [Checkbox.name]: Checkbox,\n    [Button.name]: Button,\n  },\n  data() {\n    return {\n      form: {\n        firstName: \"\",\n        lastName: \"\",\n        email: \"\",\n        password: \"\",\n        acceptTerms: true,\n      },\n    };\n  },\n  methods: {\n    toggleNavbar() {\n      document.body.classList.toggle(\"nav-open\");\n    },\n    closeMenu() {\n      document.body.classList.remove(\"nav-open\");\n      document.body.classList.remove(\"off-canvas-sidebar\");\n    },\n    register() {\n      axiosClient\n        .post(\"/register\", {\n          firstName: this.formfirstName,\n          lastName: this.form.lastName,\n          email: this.form.email,\n          password: this.form.password,\n        })\n        .then((response) => {\n          sessionStorage.setItem(\"UserID\", response.data.user.id);\n          sessionStorage.setItem(\"Token\", response.data.token);\n          sessionStorage.setItem(\"Position\", response.data.user.position);\n          this.$router.push({ name: \"Overview\" });\n        })\n        .catch((error) => {\n          console.error(error);\n        });\n    },\n  },\n  beforeDestroy() {\n    this.closeMenu();\n  },\n};\n</script>\n<style></style>\n"],"mappings":"AAmEA,OAAAA,SAAA;AAEA,SAAAC,IAAA,EAAAC,QAAA,EAAAC,MAAA,EAAAC,WAAA;AAEA;EACAC,UAAA;IACAJ,IAAA;IACAD,SAAA;IA
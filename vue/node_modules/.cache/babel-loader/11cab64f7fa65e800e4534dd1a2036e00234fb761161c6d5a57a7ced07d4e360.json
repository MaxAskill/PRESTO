n    from() {\n      return this.pagination.perPage * (this.pagination.currentPage - 1);\n    },\n    total() {\n      this.pagination.total = this.tableData.length;\n      return this.tableData.length;\n    }\n  },\n  data() {\n    return {\n      company: \"NBFI\",\n      pagination: {\n        perPage: 5,\n        currentPage: 1,\n        perPageOptions: [5, 10, 25, 50],\n        total: 0\n      },\n      searchQuery: \"\",\n      propsToSearch: [\"id\", \"brandNames\", \"status\"],\n      tableColumns: [{\n        prop: \"id\",\n        label: \"ID\",\n        minWidth: 200\n      }, {\n        prop: \"brandNames\",\n        label: \"Brand Name\",\n        minWidth: 250\n      }, {\n        prop: \"status\",\n        label: \"Status\",\n        minWidth: 100\n      }],\n      tableData: []\n    };\n  },\n  methods: {\n    fetchData() {\n      axiosClient.get(\"/fetchBrandMaintenance\", {\n        params: {\n          company: this.company\n        }\n      }).then(response => {\n        console.log(\"Brand Response:\", response.data);\n        this.tableData = response.data;\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    handleEdit(index, row) {\n      alert(`Your want to edit ${row.branchName}`);\n    },\n    handleDelete(index, row) {\n      let indexToDelete = this.tableData.findIndex(tableRow => tableRow.id === row.id);\n      if (indexToDelete >= 0) {\n        this.tableData.splice(indexToDelete, 1);\n      }\n    }\n  }\n};","map":{"version":3,"names":["Vue","PButton","PPagination","axiosClient","use","_Table","_TableColumn","_Select","_Option","components","mounted","fetchData","computed","pagedData","tableData","slice","from","to","queriedData","searchQuery","pagination","total","length","result","filter","row","isIncluded","key","propsToSearch","rowValue","toString","toLowerCase","includes","highBound","perPage","currentPage","data","company","perPageOptions","tableColumns","prop","label","minWidth","methods","get","pa
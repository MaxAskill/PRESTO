s=\"modal-dialog modal-dialog-centered\">\n      <div class=\"modal-content\">\n        <div class=\"modal-header\"></div>\n        <div class=\"modal-body text-center\" v-if=\"isSubmit\">\n          <p>\n            Your transaction has been processed successfully.<br /><br />Transaction\n            Number<br /><span class=\"tr_num\">{{ transferTransactionID }}</span\n            ><br /><br />Please take a photo or screenshot the transaction number before\n            closing this window.\n          </p>\n        </div>\n        <div class=\"modal-body text-center\" v-else>\n          <p>\n            The transaction has been reviewed successfully.<br /><br />Transaction\n            Number<br /><span class=\"tr_num\">{{ transferTransactionID }}</span\n            ><br /><br />The transaction will now proceed for approval.\n          </p>\n        </div>\n        <div class=\"modal-footer d-flex justify-content-center\">\n          <!-- <div class=\"spinner-border text-dark\" role=\"status\" v-if=\"counter\">\n            <span class=\"sr-only\">Loading...</span>\n          </div> -->\n          <!-- <div v-else> -->\n          <button\n            class=\"btn btn-secondary\"\n            data-bs-dismiss=\"modal\"\n            @click=\"reloadPage()\"\n            :disabled=\"counter\"\n          >\n            <span\n              class=\"spinner-border spinner-border-sm\"\n              role=\"status\"\n              aria-hidden=\"true\"\n              v-if=\"counter\"\n            ></span>\n            Close\n          </button>\n          <!-- </div> -->\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nimport Vue from \"vue\";\nimport axiosClient from \"../../../../../../axios\";\nimport axios from \"axios\";\n\nexport default {\n  props: [\"transferTransactionID\", \"savingCounter\"],\n  components: {},\n  watch: {\n    savingCounter: \"counterSaving\",\n  },\n  computed: {\n    /***\n     * Searches through table data and returns a paginated array.\n     * Note that this should not be used for table with a lot of data as it might be slow!\n     * Do the search and the pagination on the server and display the data retrieved from server instead.\n     * @returns {computed.pagedData}\n     */\n  },\n  data() {\n    return {\n      counter: true,\n      isSubmit: true,\n    };\n  },\n  mounted() {\n    this.isEditParagraph();\n  },\n  methods: {\n    isEditParagraph() {\n      if (\n        sessionStorage.getItem(\"Position\") == \"Reviewer\" ||\n        sessionStorage.getItem(\"Position\") == \"Approver\"\n      ) {\n        this.isSubmit = false;\n      }\n    },\n    reloadPage() {\n      // window.location.href = \"http://192.168.0.7:4040/#/pull-out/pullout-transaction\";\n\n      if (\n        sessionStorage.getItem(\"Position\") == \"Reviewer\" ||\n        sessionStorage.getItem(\"Position\") == \"Approver\"\n      )\n        this.$router.push({\n          path: \"/pull-out/requests\",\n        });\n      else\n        this.$router.push({\n          path: \"/pull-out/pullout-transaction\",\n        });\n    },\n    counterSaving() {\n      // foreach(v);\n      console.log(\"Counter: \", this.counter);\n      setTimeout(() => {\n        this.counter = false;\n      }, this.savingCounter * 1000);\n      console.log(\"Counter after: \", this.counter);\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\">\n.tr_num {\n  font-size: 35px;\n  font-weight: 700;\n}\n</style>\n"],"mappings":"AAsDA,OAAAA,GAAA;AACA,OAAAC,WAAA;AACA,OAAAC,KAAA;AAEA;EACAC,KAAA;EACAC,UAAA;EACAC,KAAA;IACAC,aAAA;EACA;EACAC,QAAA,GAOA;EACAC,IAAA,WAAAA,KAAA;IACA;MACAC,OAAA;MACAC,QAAA;IACA;EACA;EACAC,OAAA,WAAAA,QAAA;IACA,KAAAC,eAAA;EACA;EACAC,OAAA;IACAD,eAAA,WAAAA,gBAAA;MACA,IACAE,cAAA,CAAAC,OAAA,8BACAD,cAAA,CAAAC,OAAA,4BACA;QACA,KAAAL,QAAA;MACA;IACA;IACAM,UAAA,WAAAA,WAAA;MAGA,IACAF,cAAA,CAAAC,OAAA,8BACAD,cAAA,CAAAC,OAAA,4BAEA,KAAAE,OAAA,CAAAC,IAAA;QACAC,IAAA;MACA,QAEA,KAAAF,OAAA,CAAAC,IAAA;QACAC,IAAA;MACA;IACA;IACAC,aAAA,WAAAA,c
a);\n      this.dateStarted = this.dateData[0].dateStart;\n      this.dateEnded = this.dateData[0].dateEnd;\n      this.authorizedPersonnel = this.dateData[0].authorizedPersonnel;\n      console.log(\"Transfer Date:\", this.dateStarted, this.dateEnded);\n    },\n    async generateLetter() {\n      const loading = this.$loading({\n        lock: true,\n        text: \"Loading\",\n        spinner: \"el-icon-loading\",\n        background: \"rgba(0, 0, 0, 0.7)\"\n      });\n      setTimeout(() => {\n        loading.close();\n      }, 2000);\n    },\n    validateGenerate() {\n      if (this.transferredData.createdBy && this.dateStarted && this.dateEnded) this.isDisabled = false;else this.isDisabled = true;\n    },\n    convertToAlphanumeric(input) {\n      let result = \"\";\n      for (let i = 0; i < input.length; i++) {\n        const char = input[i];\n        const charCode = char.charCodeAt(0);\n        if (char >= \"0\" && char <= \"9\" || char >= \"a\" && char <= \"z\" || char >= \"A\" && char <= \"Z\") {\n          const alphanumericChar = charCode.toString(36);\n          result += alphanumericChar;\n        } else {\n          result += char;\n        }\n      }\n      return result;\n    }\n  }\n};","map":{"version":3,"names":["Vue","axiosClient","ApprovalModal","use","_Table","_TableColumn","_Select","_Option","_DatePicker","props","components","watch","computed","data","pickerOptions","shortcuts","text","onClick","picker","$emit","Date","dateStarted","dateEnded","isDisabled","authorizedPersonnel","fullscreenLoading","transferredData.name","val","oldVal","validateGenerate","dateData","handler","transferDate","deep","methods","console","log","dateStart","dateEnd","generateLetter","loading","$loading","lock","spinner","background","setTimeout","close","transferredData","createdBy","convertToAlphanumeric","input","result","i","length","char","charCode","charCodeAt","alphanumericChar","toString"],"sources":["src/components/Dashboard/Views/PullOut/PullOutRequests/M
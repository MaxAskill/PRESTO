});\n      }, 3000);\n    },\n    validateGenerate() {\n      if (this.transferredData.createdBy && this.dateStarted && this.dateEnded) this.isDisabled = false;else this.isDisabled = true;\n    },\n    convertToAlphanumeric(input) {\n      let result = \"\";\n      for (let i = 0; i < input.length; i++) {\n        const char = input[i];\n        const charCode = char.charCodeAt(0);\n        if (char >= \"0\" && char <= \"9\" || char >= \"a\" && char <= \"z\" || char >= \"A\" && char <= \"Z\") {\n          const alphanumericChar = charCode.toString(36);\n          result += alphanumericChar;\n        } else {\n          result += char;\n        }\n      }\n      return result;\n    }\n  }\n};","map":{"version":3,"names":["Vue","axiosClient","ApprovalModal","axios","use","_Table","_TableColumn","_Select","_Option","_DatePicker","props","components","watch","computed","data","pickerOptions","shortcuts","text","onClick","picker","$emit","Date","dateStarted","dateEnded","isDisabled","authorizedPersonnel","transferredData.name","val","oldVal","validateGenerate","dateData","handler","transferDate","deep","methods","console","log","dateStart","dateEnd","generateLetter","post","company","sessionStorage","getItem","id","transferredData","plID","status","userID","editStatus","then","response","catch","error","holderName","convertToAlphanumeric","holderplID","holderDateStart","holderDateEnd","holderEmail","holderUserID","holderStatus","holderCompany","holderRegenerate","window","open","createdBy","promoEmail","setTimeout","$router","push","path","input","result","i","length","char","charCode","charCodeAt","alphanumericChar","toString"],"sources":["src/components/Dashboard/Views/PullOut/PullOutRequests/ModalPullOut/PendingApprovalModal/ApprovedPendingModal.vue"],"sourcesContent":["<template>\n  <div\n    class=\"modal fade\"\n    id=\"approvedPending\"\n    data-bs-backdrop=\"static\"\n    data-bs-keyboard=\"false\"\n    tabindex=\"-1\"\n    aria-labelledby=\"approvedPendingLabel\"\n    aria-hidden=\"true\"\n  >\n    <div class=\"modal-dialog modal-dialog-centered modal-lg\">\n      <div class=\"modal-content\">\n        <div class=\"modal-header\">\n          <h1 class=\"modal-title fs-5 text-weight-bold\" id=\"approvedPendingLabel\">\n            {{ transferredData.branchName }}\n          </h1>\n        </div>\n        <div class=\"modal-body\">\n          <div class=\"row\">\n            <div class=\"col-12 pull-left\">\n              <fg-input\n                label=\"Assigned Personnel for Pull-Out\"\n                v-model=\"authorizedPersonnel\"\n              ></fg-input>\n            </div>\n            <div class=\"col-6\">\n              <fg-input label=\"Pull-Out Date Started\">\n                <el-date-picker\n                  v-model=\"dateStarted\"\n                  type=\"date\"\n                  placeholder=\"Select a Day\"\n                  :picker-options=\"pickerOptions\"\n                >\n                </el-date-picker>\n              </fg-input>\n            </div>\n            <div class=\"col-6\">\n              <fg-input label=\"Pull-Out Date Ended\">\n                <el-date-picker\n                  v-model=\"dateEnded\"\n                  type=\"date\"\n                  placeholder=\"Select a Day\"\n                  :picker-options=\"pickerOptions\"\n                >\n                </el-date-picker>\n              </fg-input>\n            </div>\n          </div>\n        </div>\n        <div class=\"modal-footer px-5\">\n          <button class=\"btn btn-secondary\" data-bs-dismiss=\"modal\">Close</button>\n          <button\n            :disabled=\"isDisabled\"\n            type=\"button\"\n            class=\"btn btn-success\"\n            data-bs-dismiss=\"modal\"\n            @click=\"generateLetter\"\n          >\n            Generate Letter\n          </button>\n        </div>\n      </div>\n    </div>\n    <ApprovalModal></ApprovalModal>\n  </div>\n</template>\n<script>\nimport Vue from 
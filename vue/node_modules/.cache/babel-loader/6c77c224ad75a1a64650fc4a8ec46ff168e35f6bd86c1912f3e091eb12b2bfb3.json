dashboard\"\n                    v-model.trim=\"edit.Name\"\n                  >\n                  </fg-input>\n                </div>\n                <div class=\"col-md-6\">\n                  <fg-input\n                    v-show=\"!editMode\"\n                    type=\"text\"\n                    label=\"Email\"\n                    placeholder=\"Username\"\n                    v-model.trim=\"user.email\"\n                    disabled\n                  >\n                  </fg-input>\n                  <fg-input\n                    v-show=\"editMode\"\n                    type=\"text\"\n                    label=\"Email\"\n                    placeholder=\"Username\"\n                    v-model.trim=\"edit.Email\"\n                  >\n                  </fg-input>\n                </div>\n              </div>\n\n              <div class=\"row\" v-show=\"!editMode\" v-for=\"user in userMultiple\">\n                <div class=\"col-md-3\">\n                  <span class=\"headerBranch m-2\">Company</span>\n                  <fg-input\n                    type=\"text\"\n                    placeholder=\"Company\"\n                    v-model=\"user.company\"\n                    disabled\n                  >\n                  </fg-input>\n                </div>\n                <div class=\"col-md-3\">\n                  <fg-input\n                    type=\"text\"\n                    label=\"Chain Code\"\n                    placeholder=\"Chain Code\"\n                    v-model=\"user.chainCode\"\n                    disabled\n                  >\n                  </fg-input>\n                </div>\n                <div class=\"col-md-6\">\n                  <fg-input\n                    type=\"text\"\n                    label=\"Branch Name\"\n                    placeholder=\"Branch Name\"\n                    v-model=\"user.branchName\"\n                    disabled\n                  >\n                  </fg-input>\n                </div>\n    
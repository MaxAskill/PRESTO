{"ast":null,"code":"import \"core-js/modules/es.regexp.exec.js\";\nimport \"core-js/modules/es.string.replace.js\";\nimport UnprocessModal from \"../UnprocessModal.vue\";\nexport default {\n  components: {\n    UnprocessModal: UnprocessModal\n  },\n  props: [\"transferredData\", \"viewImages\"],\n  data: function data() {\n    return {\n      slideIndex: 1\n    };\n  },\n  methods: {\n    plusSlides: function plusSlides(n) {\n      showSlides(slideIndex += n);\n    },\n    currentSlide: function currentSlide(n) {\n      showSlides(slideIndex = n);\n    },\n    showSlides: function showSlides(n) {\n      var i;\n      var slides = document.getElementsByClassName(\"mySlides\");\n      var dots = document.getElementsByClassName(\"demo\");\n      var captionText = document.getElementById(\"caption\");\n      if (n > slides.length) {\n        slideIndex = 1;\n      }\n      if (n < 1) {\n        slideIndex = slides.length;\n      }\n      for (i = 0; i < slides.length; i++) {\n        slides[i].style.display = \"none\";\n      }\n      for (i = 0; i < dots.length; i++) {\n        dots[i].className = dots[i].className.replace(\" active\", \"\");\n      }\n      slides[slideIndex - 1].style.display = \"block\";\n      dots[slideIndex - 1].className += \" active\";\n      captionText.innerHTML = dots[slideIndex - 1].alt;\n    }\n  }\n};","map":{"version":3,"names":["UnprocessModal","components","props","data","slideIndex","methods","plusSlides","n","showSlides","currentSlide","i","slides","document","getElementsByClassName","dots","captionText","getElementById","length","style","display","className","replace","innerHTML","alt"],"sources":["src/components/Dashboard/Views/PullOut/PullOutRequests/ModalPullOut/UnprocessModal/ViewModal.vue"],"sourcesContent":["<template>\n  <div>\n    <div\n      class=\"modal fade\"\n      id=\"viewModal\"\n      data-bs-backdrop=\"static\"\n      data-bs-keyboard=\"false\"\n      tabindex=\"-1\"\n      aria-labelledby=\"viewModalLabel\"\n      aria-hidden=\"true\"\n    >\n      <div class=\"modal-dialog modal-dialog-centered modal-lg\">\n        <div class=\"modal-content\">\n          VIEWING\n          <div>\n            <!-- <img v-for=\"image in viewImages\" :src=\"image\" /> -->\n            <!-- <div v-for=\"(path, index) in viewImages\" :key=\"index\">\n            <img :src=\"path\" :alt=\"'Image ' + (index + 1)\" />\n          </div> -->\n            <!-- <img\n            src=\"http://192.168.0.7:40/public/uploads/NBFI/122-NCCC DEPT STORE PALAWAN-1689041208.png\"\n            alt=\"Image\"\n          /> -->\n          </div>\n          <div class=\"container\">\n            <!-- Full-width images with number text -->\n            <div class=\"mySlides\" v-for=\"(path, index) in viewImages\" :key=\"index\">\n              <!-- <div class=\"numbertext\">{{ index }}</div> -->\n              <img :src=\"path\" :alt=\"'Image ' + (index + 1)\" style=\"width: 100%\" />\n            </div>\n            <!-- Trigger the Modal -->\n            <img\n              id=\"myImg\"\n              src=\"img_snow.jpg\"\n              alt=\"Snow\"\n              style=\"width: 100%; max-width: 300px\"\n            />\n\n            <!-- The Modal -->\n            <div id=\"myModal\" class=\"modal\">\n              <!-- The Close Button -->\n              <span class=\"close\">&times;</span>\n\n              <!-- Modal Content (The Image) -->\n              <img class=\"modal-content\" id=\"img01\" />\n\n              <!-- Modal Caption (Image Text) -->\n              <div id=\"caption\"></div>\n            </div>\n          </div>\n          <div class=\"modal-footer mrgn-footer\">\n            <button type=\"button\" class=\"btn btn-secondary\" data-bs-dismiss=\"modal\">\n              Close\n            </button>\n            <button\n              type=\"button\"\n              class=\"btn btn-primary\"\n              data-bs-target=\"#unprocessModal\"\n              data-bs-toggle=\"modal\"\n            >\n              Back\n            </button>\n          </div>\n        </div>\n      </div>\n    </div>\n    <UnprocessModal></UnprocessModal>\n  </div>\n</template>\n<script>\nimport UnprocessModal from \"../UnprocessModal.vue\";\n\nexport default {\n  components: {\n    UnprocessModal,\n  },\n  props: [\"transferredData\", \"viewImages\"],\n  data() {\n    return {\n      slideIndex: 1,\n    };\n  },\n  methods: {\n    plusSlides(n) {\n      showSlides((slideIndex += n));\n    },\n    currentSlide(n) {\n      showSlides((slideIndex = n));\n    },\n    showSlides(n) {\n      let i;\n      let slides = document.getElementsByClassName(\"mySlides\");\n      let dots = document.getElementsByClassName(\"demo\");\n      let captionText = document.getElementById(\"caption\");\n      if (n > slides.length) {\n        slideIndex = 1;\n      }\n      if (n < 1) {\n        slideIndex = slides.length;\n      }\n      for (i = 0; i < slides.length; i++) {\n        slides[i].style.display = \"none\";\n      }\n      for (i = 0; i < dots.length; i++) {\n        dots[i].className = dots[i].className.replace(\" active\", \"\");\n      }\n      slides[slideIndex - 1].style.display = \"block\";\n      dots[slideIndex - 1].className += \" active\";\n      captionText.innerHTML = dots[slideIndex - 1].alt;\n    },\n  },\n};\n</script>\n\n<style>\n/* Style the Image Used to Trigger the Modal */\n#myImg {\n  border-radius: 5px;\n  cursor: pointer;\n  transition: 0.3s;\n}\n\n#myImg:hover {\n  opacity: 0.7;\n}\n\n/* The Modal (background) */\n.modal {\n  display: none; /* Hidden by default */\n  position: fixed; /* Stay in place */\n  z-index: 1; /* Sit on top */\n  padding-top: 100px; /* Location of the box */\n  left: 0;\n  top: 0;\n  width: 100%; /* Full width */\n  height: 100%; /* Full height */\n  overflow: auto; /* Enable scroll if needed */\n  background-color: rgb(0, 0, 0); /* Fallback color */\n  background-color: rgba(0, 0, 0, 0.9); /* Black w/ opacity */\n}\n\n/* Modal Content (Image) */\n.modal-content {\n  margin: auto;\n  display: block;\n  width: 80%;\n  max-width: 700px;\n}\n\n/* Caption of Modal Image (Image Text) - Same Width as the Image */\n#caption {\n  margin: auto;\n  display: block;\n  width: 80%;\n  max-width: 700px;\n  text-align: center;\n  color: #ccc;\n  padding: 10px 0;\n  height: 150px;\n}\n\n/* Add Animation - Zoom in the Modal */\n.modal-content,\n#caption {\n  animation-name: zoom;\n  animation-duration: 0.6s;\n}\n\n@keyframes zoom {\n  from {\n    transform: scale(0);\n  }\n  to {\n    transform: scale(1);\n  }\n}\n\n/* The Close Button */\n.close {\n  position: absolute;\n  top: 15px;\n  right: 35px;\n  color: #f1f1f1;\n  font-size: 40px;\n  font-weight: bold;\n  transition: 0.3s;\n}\n\n.close:hover,\n.close:focus {\n  color: #bbb;\n  text-decoration: none;\n  cursor: pointer;\n}\n\n/* 100% Image Width on Smaller Screens */\n@media only screen and (max-width: 700px) {\n  .modal-content {\n    width: 100%;\n  }\n}\n</style>\n"],"mappings":";;AAsEA,OAAAA,cAAA;AAEA;EACAC,UAAA;IACAD,cAAA,EAAAA;EACA;EACAE,KAAA;EACAC,IAAA,WAAAA,KAAA;IACA;MACAC,UAAA;IACA;EACA;EACAC,OAAA;IACAC,UAAA,WAAAA,WAAAC,CAAA;MACAC,UAAA,CAAAJ,UAAA,IAAAG,CAAA;IACA;IACAE,YAAA,WAAAA,aAAAF,CAAA;MACAC,UAAA,CAAAJ,UAAA,GAAAG,CAAA;IACA;IACAC,UAAA,WAAAA,WAAAD,CAAA;MACA,IAAAG,CAAA;MACA,IAAAC,MAAA,GAAAC,QAAA,CAAAC,sBAAA;MACA,IAAAC,IAAA,GAAAF,QAAA,CAAAC,sBAAA;MACA,IAAAE,WAAA,GAAAH,QAAA,CAAAI,cAAA;MACA,IAAAT,CAAA,GAAAI,MAAA,CAAAM,MAAA;QACAb,UAAA;MACA;MACA,IAAAG,CAAA;QACAH,UAAA,GAAAO,MAAA,CAAAM,MAAA;MACA;MACA,KAAAP,CAAA,MAAAA,CAAA,GAAAC,MAAA,CAAAM,MAAA,EAAAP,CAAA;QACAC,MAAA,CAAAD,CAAA,EAAAQ,KAAA,CAAAC,OAAA;MACA;MACA,KAAAT,CAAA,MAAAA,CAAA,GAAAI,IAAA,CAAAG,MAAA,EAAAP,CAAA;QACAI,IAAA,CAAAJ,CAAA,EAAAU,SAAA,GAAAN,IAAA,CAAAJ,CAAA,EAAAU,SAAA,CAAAC,OAAA;MACA;MACAV,MAAA,CAAAP,UAAA,MAAAc,KAAA,CAAAC,OAAA;MACAL,IAAA,CAAAV,UAAA,MAAAgB,SAAA;MACAL,WAAA,CAAAO,SAAA,GAAAR,IAAA,CAAAV,UAAA,MAAAmB,GAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}